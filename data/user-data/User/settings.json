{
    "vscode_custom_css.imports": [],
    "extensions.ignoreRecommendations": true,
    "window.titleBarStyle": "custom",
    "security.workspace.trust.untrustedFiles": "open",
    "window.commandCenter": true,
    "workbench.colorTheme": "Enchant - High Contrast",
    "editor.minimap.showSlider": "always",
    "terminal.integrated.env.windows": {},
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "editor.inlineSuggest.enabled": true,
    "editor.fontFamily": "'JetbrainsMono Nerd Font', Consolas, 'Courier New', monospace",
    "editor.fontSize": 12.8,
    "editor.codeLensFontFamily": "'JetbrainsMono Nerd Font', Consolas, 'Courier New', monospace",
    "scm.inputFontFamily": "'JetbrainsMono Nerd Font', Consolas, 'Courier New', monospace",
    "editor.inlayHints.fontFamily": "'JetbrainsMono Nerd Font', Consolas, 'Courier New', monospace",
    "terminal.integrated.fontFamily": "'JetbrainsMono Nerd Font', Consolas, 'Courier New', monospace",
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    },
    "git.autofetch": true,
    "cSpell.enableFiletypes": [
        "!jsonc"
    ]
}